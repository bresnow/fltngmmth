{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
      "exports": {
        ".": {
          "types": "./remix/build.d.ts",
          "default": "./remix/build/index.js"
        },
        "./package.json": "./package.json"
      },
  "scripts": {
    "--0": "-------Remix--------",
    "remix": "yarn workspace frontend",
    "server": "yarn workspace server",
    "--1": "------- Docker--------",
    "docker": "concurrently \"yarn front:build --push=true\" \"yarn relay:build --push=true\" \"yarn dev:build --push=true\" \"yarn latest:build\"",
    "front:build": "npx zx packages/scripts/docker/build.mjs --target frontend",
    "relay:build": "",
    "latest:build": "concurrently \"yarn front:build --version edge --push=true\" \"yarn relay:build --version edge --push=true\" \"yarn dev:build --version edge --push=true\"",
    "dev:build": "npx zx packages/scripts/docker/build.mjs --target dev"
  },
  "dependencies": {
    "@picocss/pico": "^1.5.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/server-runtime": "^1.7.0",
    "@web3-storage/multipart-parser": "^1.0.0",
    "chainlocker": "^1.3.2",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "gun": "^0.2020.1238",
    "isbot": "^3.4.5",
    "jsesc": "^3.0.2",
    "lz-string": "^1.4.4",
    "morgan": "^1.10.0",
    "prismjs": "^1.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redaxios": "^0.5.1",
    "remix": "^1.7.0",
    "super-expressive": "^1.4.1",
    "@remix-run/express": "^1.7.0",
    "express": "^4.18.1",
    "@remix-validated-form/with-zod": "^2.0.1",
    "ariakit": "^2.0.0-next.40",
    "remix-validated-form": "^4.5.5",
    "zod": "^3.18.0",
    "zod-form-data": "^1.2.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@remix-run/node": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@types/jsesc": "^3.0.1",
    "@types/lz-string": "^1.3.34",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
