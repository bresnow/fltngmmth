{
  "name": "remix-app",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build.d.ts",
      "default": "./build/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"npm run build:css\" \"npm run build:remix\"",
    "build:css": "npm run generate:css",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:css": "npm run generate:css --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "generate:css": "unocss app/**/*.tsx ../../ui/**/*.tsx ../../ui/**/*.ts --out-file ./app/uno.css",
    "postinstall": "remix setup node",
    "prettier": "prettier --write '**/*.{json,md,mdx,mjs,mts,js,jsx,ts,tsx}'",
    "lint": "eslint .",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@picocss/pico": "^1.5.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/server-runtime": "^1.7.0",
    "@web3-storage/multipart-parser": "^1.0.0",
    "chainlocker": "^1.3.2",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "gun": "^0.2020.1238",
    "isbot": "^3.4.5",
    "lz-string": "^1.4.4",
    "morgan": "^1.10.0",
    "prismjs": "^1.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redaxios": "^0.5.1",
    "remix": "^1.7.0",
    "super-expressive": "^1.4.1"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@remix-run/node": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@types/lz-string": "^1.3.34",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@unocss/cli": "^0.38.2",
    "@unocss/preset-attributify": "^0.38.2",
    "@unocss/preset-icons": "^0.45.12",
    "@unocss/preset-typography": "^0.38.2",
    "@unocss/reset": "^0.45.13",
    "@unocss/transformer-directives": "^0.45.13",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "duecss": "^0.27.0",
    "eslint": "^8.15.0",
    "eslint-config-custom": "*",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tsconfig": "*",
    "typescript": "^4.6.4",
    "unocss": "^0.45.12"
  }
}
